{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft.Hosting.Lifetime": "Information",
        "Microsoft": "Debug",
        "System": "Warning",
        "Microsoft.Extensions.Diagnostics.HealthChecks": "Error",
        "LettuceEncrypt": "Verbose"
      }
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Default": "filename=database.db;password=1234;Mode=Exclusive"
  },
  "GlobalCache": {
    "Provider": "Memory"
  }
}
